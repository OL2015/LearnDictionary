<Window x:Class="DictionaryUI.View.VisualTreeDisplay"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DictionaryUI.View"
        mc:Ignorable="d"
        Title="VisualTreeDisplay" Height="300" Width="300">
    <StackPanel Orientation="Vertical">
        <TreeView Name="VisualTreeElements" Margin="10"></TreeView>

        <TreeView Name="LogicalTreeElements" Margin="10"></TreeView>
    </StackPanel>
</Window>
